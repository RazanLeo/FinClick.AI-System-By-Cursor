{
  "name": "finclick-ai-system",
  "version": "1.0.0",
  "description": "Revolutionary Intelligent Financial Analysis System with 170+ analysis types",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && python main.py",
    "dev:frontend": "cd frontend && npm start",
    "dev:ai": "cd ai_engine && python3 simple_demo.py",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && pip install -r requirements.txt",
    "start": "npm run start:backend",
    "start:backend": "cd backend && python main.py",
    "start:frontend": "cd frontend && npm start",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && python -m pytest",
    "test:frontend": "cd frontend && npm test",
    "lint": "npm run lint:backend && npm run lint:frontend",
    "lint:backend": "cd backend && black . && isort . && flake8 .",
    "lint:frontend": "cd frontend && npm run lint",
    "format": "npm run format:backend && npm run format:frontend",
    "format:backend": "cd backend && black . && isort .",
    "format:frontend": "cd frontend && npm run format",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "setup": "npm run setup:backend && npm run setup:frontend",
    "setup:backend": "cd backend && pip install -r requirements.txt",
    "setup:frontend": "cd frontend && npm install",
    "clean": "npm run clean:backend && npm run clean:frontend",
    "clean:backend": "cd backend && find . -type d -name __pycache__ -exec rm -rf {} + && find . -type f -name '*.pyc' -delete",
    "clean:frontend": "cd frontend && rm -rf node_modules build",
    "deploy:vercel": "vercel --prod",
    "deploy:docker": "npm run docker:build && npm run docker:up"
  },
  "keywords": [
    "financial-analysis",
    "ai",
    "machine-learning",
    "fintech",
    "investment",
    "stock-analysis",
    "financial-modeling",
    "arabic",
    "bilingual",
    "fastapi",
    "react",
    "typescript",
    "python"
  ],
  "author": "FinClick.AI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RazanLeo/FinClick.AI-System-By-Cursor.git"
  },
  "bugs": {
    "url": "https://github.com/RazanLeo/FinClick.AI-System-By-Cursor/issues"
  },
  "homepage": "https://finclick.ai",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0",
    "python": ">=3.11.0"
  },
  "workspaces": [
    "frontend",
    "backend",
    "ai_engine"
  ]
}
